{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["activeTab", "tabs", "declarativeContent", "storage", "background"],
  "options_page": "options.html",
  "background": {
    "matches": ["<all_urls>"],
    "scripts": ["background.js"],
    "persistent": true
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/subSaver_Logo_copy.png",
      "32": "images/subSaver_Logo_copy.png",
      "48": "images/subSaver_Logo_copy.png",
      "128": "images/subSaver_Logo_copy.png"
    }
  },
  "icons": {
    "16": "images/subSaver_Logo_copy.png",
    "32": "images/subSaver_Logo_copy.png",
    "48": "images/subSaver_Logo_copy.png",
    "128": "images/subSaver_Logo_copy.png"
  },
  "manifest_version": 2
}
