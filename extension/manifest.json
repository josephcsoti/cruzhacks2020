{
  "name": "SubSaver",
  "version": "1.0",
  "description": "Save on your Subscriptions!",
  "permissions": ["activeTab", "tabs", "declarativeContent", "storage", "background", "https://code.jquery.com/jquery-3.4.1.slim.min.js", "https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js", "https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"],
  "options_page": "options.html",
  "background": {
    "matches": ["<all_urls>"],
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_security_policy": "script-src 'self' https://code.jquery.com/jquery-3.4.1.slim.min.js https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js 'unsafe-inline' 'unsafe-eval'",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/subSaver_Logo_copy.png",
      "32": "images/subSaver_Logo_copy.png",
      "48": "images/subSaver_Logo_copy.png",
      "128": "images/subSaver_Logo_copy.png"
    }
  },
  "icons": {
    "16": "images/subSaver_Logo_copy.png",
    "32": "images/subSaver_Logo_copy.png",
    "48": "images/subSaver_Logo_copy.png",
    "128": "images/subSaver_Logo_copy.png"
  },
  "manifest_version": 2
}
